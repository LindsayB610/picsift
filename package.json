{
  "name": "picsift",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:functions": "netlify dev",
    "build": "npm run build:functions && vite build",
    "build:functions": "tsc --noEmit -p tsconfig.functions.json",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit && tsc --noEmit -p tsconfig.functions.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:a11y": "vitest run src/test/a11y",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.7",
    "@netlify/blobs": "^10.5.0",
    "@tanstack/react-query": "^5.90.20",
    "canvas-confetti": "^1.9.4",
    "dotenv": "^17.2.3",
    "dropbox": "^10.34.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.3.3",
    "axe-core": "^4.10.2",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^15.12.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.3.3",
    "typescript": "~5.6.3",
    "vite": "^5.4.10",
    "@vitest/coverage-v8": "^2.1.6",
    "vitest": "^2.1.6",
    "vitest-axe": "^0.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
